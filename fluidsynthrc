set audio.driver jack
set audio.file.type wav
set audio.periods 3 
set audio.period-size 256 
set audio.file.format 32
set audio.jack.id FluidSynth 

set midi.driver alsa_seq
set midi.alsa_seq.id FluidSynth 
set midi.jack.id FluidSynth 

#set synth.sample-rate 48000 #BARDEA MAL
set synth.midi-bank-select gm 
set shell.port 1301

reverb 1
rev_preset 1
chorus 1

